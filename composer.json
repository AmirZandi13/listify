{
    "name": "lookitsatravis/listify",
    "description": "Turn any Eloquent model into a list! http://lookitsatravis.github.io/listify",
    "keywords": ["laravel", "model", "position", "acts_as_list", "rails", "list", "eloquent", "order"],
    "license": "MIT",
    "authors": [{
        "name": "Travis Vignon",
        "email": "travis@lookitsatravis.com",
        "homepage": "http://lookitsatravis.com"
    }],
    "require": {
        "php": ">=5.4.0",
        "illuminate/database": "4.*",
        "illuminate/support": "4.*",
        "illuminate/console": "4.*",
        "illuminate/events": "4.*"
    },
    "require-dev": {
        "satooshi/php-coveralls": "dev-master",
        "way/phpunit-wrappers": "dev-master"
    },
    "autoload": {
        "psr-0": {
            "Lookitsatravis\\Listify": "src/"
        }
    },
    "autoload-dev": {
        "classmap": ["tests/models"]
    },
    "minimum-stability": "stable"
}